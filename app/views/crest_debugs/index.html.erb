<section class="content-header">
  <h1>Crest Debug</h1>
</section>

<section class="content">
  <div class="box">
    <div class="box-body">

      <div class="form-group">
        <label>URL:</label>
        <%= text_field :crest_url, :url, value: @init_url, :size => "100" %>
        <%= button_tag "get", :id => "get_crest_button" %>
      </div>

      <div id="crest_result_area">
        <%= render :partial => 'crest_result' %>
      </div>
    </div>
  </div>
</section>
